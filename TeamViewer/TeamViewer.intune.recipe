		<key>developer</key>
		<string>TeamViweer GmbH</string>
		<key>display_name</key>
		<string>TeamViewer</string>
		<key>postinstallscript</key>
		<string>IyEvYmluL3NoCiMjIHBvc3RpbnN0YWxsIHNjcmlwdAoKIyBDbGVhbiB0aGUgdG1wIGZpbGUgY3JlYXRlZCBhdCBwcmVpbnN0YWxsIHBoYXNlLgoKc3VkbyBybSAtcmYgL3RtcC90dm9ubHlzdGFydHNlcnZpY2U=</string>
